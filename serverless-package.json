{
  "name": "ucsc-eats-api",
  "version": "1.0.0",
  "description": "UCSC Eats API using AWS Serverless",
  "main": "index.js",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "offline": "serverless offline",
    "migrate": "serverless invoke --function migrateData"
  },
  "dependencies": {
    "aws-sdk": "^2.1490.0"
  },
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.0"
  }
}
